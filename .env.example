# Solana private key in base58 format
SOLANA_PRIVATE_KEY=

# RPC Endpoint (optional, defaults to Solana mainnet public RPC)
SOLANA_RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Network to use: 'mainnet-beta', 'testnet', or 'devnet' (optional, defaults to mainnet-beta)
SOLANA_NETWORK=mainnet-beta

# Security - optional API key for additional protection
API_KEY=

# Logging level: 'error', 'warn', 'info', 'debug' (optional, defaults to 'info')
LOG_LEVEL=info
